@model IEnumerable<MVCUI.ViewModels.Account.UserViewModel>

@{
    ViewBag.Title = "Users";
}
<h3>All users</h3>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Role)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayTextFor(modelItem => item.Role.Name)
            </td>
        </tr>
    }
</table>